{
    "format":
    {
        "name":         "The TBOOX Package Format"
    ,   "version":      "v1.0.1"
    ,   "website":      "http://www.tboox.org"
    }

,   "package":
    {
        "name":         "The Skia Library"
    ,   "website":      "https://code.google.com/p/skia/"
    }

,   "compiler": 
    {
        "default":
        {
            "debug":
            {
                "libs":         "skia"
            ,   "libpath":      ""
            ,   "incpath":      ""
            ,   "libflags":     ""
            ,   "incflags":     ""
            }
        ,   "release":          "$.compiler.default.debug"
        }

    ,   "mac":
        {
            "x64":
            {
                "debug":
                {
                    "libs":         "skia"
                ,   "libpath":      ""
                ,   "incpath":      ""
                ,   "libflags":     "-framework AppKit -framework AGL"
                ,   "incflags":     ""
                }
            }
        }
    }
}
